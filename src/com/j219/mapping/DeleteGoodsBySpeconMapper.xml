<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.j219.dao.DeleteGoodsBySpeconMapper">
    
    <delete id="deleteGoodsG">
    	delete from goods where goods_no=#{goods_no};
    </delete>
    
    <delete id="deleteGoodsS">
    	delete from spec where spec_no=#{spec_no};
    </delete>
    
    <delete id="deleteGoodsGd">
    	delete from goods_details where spec_no=#{spec_no};
    </delete>
    
    
</mapper>